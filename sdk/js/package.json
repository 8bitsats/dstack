{
  "name": "@phala/dstack-sdk",
  "version": "0.1.7",
  "description": "DStack SDK",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./viem": {
      "import": "./dist/viem.js",
      "require": "./dist/viem.js",
      "types": "./dist/viem.d.ts"
    },
    "./encrypt-env-vars": {
      "import": "./dist/encrypt-env-vars.js",
      "require": "./dist/encrypt-env-vars.js",
      "types": "./dist/encrypt-env-vars.d.ts"
    }
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:ci": "vitest --run",
    "release": "npm run build && npm publish --access public"
  },
  "keywords": ["tappd", "sdk"],
  "author": "Leechael Yim",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/node": "latest",
    "typescript": "latest",
    "vitest": "^2.1.3"
  },
  "optionalDependencies": {
    "viem": "^2.21.0 <3.0.0",
    "@noble/curves": "^1.8.1"
  },
  "peerDependencies": {
    "viem": "^2.21.0 <3.0.0",
    "@noble/curves": "^1.8.1"
  },
  "peerDependenciesMeta": {
    "viem": {
      "optional": true
    },
    "@noble/curves": {
      "optional": true
    }
  }
}
